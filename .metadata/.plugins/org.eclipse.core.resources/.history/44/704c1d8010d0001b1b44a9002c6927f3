
public class Bonus {
	public static void main(String[] args) {
		int total=0;
		int dec ;
		int a = 1;
		System.out.print(a + " ");
		for(;a<=5;a++) {
			for(dec=5;dec>0;dec--) {
				total +=a;
				total +=dec;
					if(total>15) {
						break;
					}
				System.out.print(total + " ");
				
			}
		}
		}
	}

