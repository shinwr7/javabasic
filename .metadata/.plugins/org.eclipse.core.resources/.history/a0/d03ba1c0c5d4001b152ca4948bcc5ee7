package interaction.game;

public class Main {
	public static void main(String[] args) {
		Dragon a = new Dragon() ;
		Warrior b = new Warrior() ;
		
		b.huntDragon(a);
		a.battleWithWarrior(b);
		b.huntDragon(a);
		a.battleWithWarrior(b);
		b.huntDragon(a);
		a.battleWithWarrior(b);
		b.huntDragon(a);
		a.battleWithWarrior(b);
		b.huntDragon(a);
		a.battleWithWarrior(b);
		b.huntDragon(a);
		a.battleWithWarrior(b);
		b.huntDragon(a);
		a.battleWithWarrior(b);
		b.huntDragon(a);
		a.battleWithWarrior(b);
		b.huntDragon(a);
		a.battleWithWarrior(b);
		b.huntDragon(a);
		a.battleWithWarrior(b);
		b.getWarriorInfo();
		a.getDragonInfo();
	}
}
