package heapmemory;

public class ArrayComparison {
	public static void main(String[] args) {
		int[] arr1 = {1, 2, 3, 4, 5};
		int[] arr2 = arr1; // 참조형 변수의 특징! 힙에 있는 데이터의 주소값만 복사해서 대입. -> 얕은 복사
						   // 반대는 깊은 복사! deep copy
		int[] arr3 = {1, 2, 3, 4, 5};
		
		arr1[0] = 10;
		System.out.println(arr1);
		System.out.println(arr2);
		System.out.println(arr3);
		
		
		
	}
}
