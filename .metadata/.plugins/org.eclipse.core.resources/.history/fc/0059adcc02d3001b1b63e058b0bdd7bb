package inheritance.game;

public class Main1 {
	public static void main(String[] args) {
		// 초보자계정 하나를 생성하고 확인해주세요.
		
		Commoner p1 = new Commoner();
		p1.id = "람";
		p1.lev = 1; 
		p1.hp = 100;
		p1.mp = 50;
		p1.getInfo();
		
		Warrior w1 = new Warrior();
		w1.id = "람";
		w1.lev = 1;
		w1.hp = 100;
		w1.mp = 50;
		w1.damage = 5;
		
		
		
	}
	
}
