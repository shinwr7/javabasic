package accessmodifier.game;

public class ProtectedWarrior extends ProtectedCommoner{
	
	public ProtectedWarrior () {
		if(hp<0) {
			System.out.println("사망");
		}
		
		if(exp>100) {
			lev+=1;
		}
	}
	
	public void hunt () {
		hp -=2;
		exp +=5;
		
	}
	public void doubleatk( ) {
		hp-=4;
		mp-=5;
		exp+=10;
		System.out.println("-----더블어택 사용-----");
		
		
	}
	
}
