package accessmodifier.game;

public class Magician extends Commoner{
	
	public Magician () {
		setHp(getHp()-30);
		setMp(getMp()+100);
		setAtk(getAtk()-5);
		
	}
	
	public void Fireball () {
		if(getMp()-20<0) {
			System.out.println("마나 부족");
		}
		else {
			setMp(getMp()-20);
			setExp(getExp()+15);
		}
	}
	
}
