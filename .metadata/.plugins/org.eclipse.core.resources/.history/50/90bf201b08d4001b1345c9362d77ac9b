package accessmodifier.game;

public class Main {
	public static void main(String[] args) {
		Warrior w1 = new Warrior ();
		
		w1.training();
		w1.training();
		w1.getAllInfo();
		w1.doubleAttack();
		w1.getAllInfo();
		
		Magician m1 = new Magician();
		
		m1.training();
		m1.getAllInfo();
		m1.Fireball();
		m1.getAllInfo();
		m1.Fireball();
		m1.Fireball();
		m1.Fireball();
		m1.Fireball();
		m1.Fireball();
	
	}
}
