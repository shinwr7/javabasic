package accessmodifier.game;

public class Warrior extends Commoner{
	
	public Warrior () {
		if(gethpInt()<=0) {
			System.out.println("체력 부족으로 스킬 사용 불가");
		}
	}
	public void doubleAttack () {
		
		System.out.println("-----전사 더블어택 사용-----");
		doubleatksetA();
		doubleatksetM();
		doubleatksetE();
		doubleatksetH();
		System.out.println("스킬 데미지 :" + getatkInt());
		System.out.println("현재 체력 : "+gethpInt());
		System.out.println("현재 마나 :" +getmpInt());
		System.out.println("현재 경험치 :" +getexpInt());
		System.out.println("현재 레벨 :" + getlevInt());
	}
}

