package accessmodifier3;

import accessmodifier2.Parent;

public class Child2 extends Parent{
	// Child2는 accessmodifier3 패키지 소속
	// Parent 는 accessmodifier2 패키지 소속으로
	// 부모 자식간 클래스 소속이 다르다.
	protected int e;
	
	public void getB() {
		System.out.println(b);
	}
//	public int a = 1;
//	protected int b = 2;
//	private int c = 3;
//	int d = 4;
	
}
